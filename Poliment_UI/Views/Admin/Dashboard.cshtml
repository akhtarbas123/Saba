@model Poliment_UI.Models.AdminDashboard
@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style type="text/css">
     .clsSetHeaderDesc{
    margin-top: 35px;
    margin-left: 195px;
    }
</style>
<h2 class="clsSetHeaderDesc">Dashboard</h2>
<br /><br />
<div>
   @if (Model != null)
   {
    <table cellspacing="20" cellpadding="20">
        <tr>
            <td><h3><span>Total voice call Sent : @Model.TotalVoiceCall</span></h3></td>
            <td><h3><span>Total Message Sent : @Model.TotalMessage</span></h3></td>
            <td><h3><span>Total space has taken by Videos : @Model.TotalVideoSpace &nbsp;GB</span></h3></td>
        </tr>
    </table>
   }
     
</div>