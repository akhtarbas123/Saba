@model Poliment_DL.Model.AdminDevelopmentWorkML
@{
    ViewBag.Title = "ShowDevelopmentWork";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/css/Common.css" rel="stylesheet" />
<style type="text/css">
    .clsSetHeaderDesc {
        margin-top: 35px;
        margin-left: 195px;
    }
</style>
<h2 class="clsSetHeaderDesc">Show Development Work</h2>
<br />
<div>
    <fieldset>
        <legend><b>Development Work</b></legend>

        <table class="clsTableLeftMargin" cellspacing="10" cellpadding="10">

            <tr>
                <td><b> Development Work Heading</b></td>
                <td>:</td>
                <td>@Model.DevelopmentHeading</td>
            </tr>
            <tr>
                <td><b>Development Work Description</b></td>
                <td>:</td>
                <td>@Model.DevelopmentDescription</td>
            </tr>

        </table>

    </fieldset>
</div>
